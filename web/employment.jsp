<script>
    var swiper = new Swiper('.swiper-container2', {
        pagination: '.swiper-pagination',
        slidesPerView: 6,
        paginationClickable: true,
        loop: true,
        spaceBetween: 30,
        autoplay: true,
        speed: 3000,
    });
    //鼠标覆盖停止自动切换
    swiper.container[0].onmouseover = function () {
        swiper.stopAutoplay();
    }
    //鼠标移出开始自动切换
    swiper.container[0].onmouseout = function () {
        swiper.startAutoplay();
    }
</script>
